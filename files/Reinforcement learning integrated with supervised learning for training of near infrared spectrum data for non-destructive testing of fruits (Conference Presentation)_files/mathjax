function SPIEMathjaxTypeset(){const t=5e3,i=(new Date).getTime(),n=setInterval(()=>{if((new Date).getTime()-i>t){clearInterval(n);return}window.MathJax&&window.MathJax.typesetPromise&&window.MathJax.typesetPromise().then(()=>{clearInterval(n)}).catch(t=>{clearInterval(n),console.error("MathJax typesetting error:",t)})},25)}function loadMathJax(){window.MathJax||(window.MathJax={loader:{load:["ui/lazy","ui/safe"]},svg:{fontCache:"local"},tex:{macros:{sb:["{_ #1}",1],sp:["{^ #1}",1]}},options:{safeOptions:{allow:{URLs:"safe"},safeProtocols:{http:!0,https:!0,file:!1,javascript:!1,data:!1}}}});const n=document.createElement("script");n.src="/Scripts/MathJax-3.2.2/es5/mml-chtml.js";n.async=!0;document.head.appendChild(n)}loadMathJax()